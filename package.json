{
  "name": "cmsc-636-project",
  "version": "1.0.0",
  "description": "VizSec Semester Project for CMSC 636 / Data Visualization",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:esturcke/cmsc-636-project.git",
    "type": "git"
  },
  "author": "Erik J. Sturcke <erik@sturcke.co>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "mongo": "mongod --bind_ip localhost --dbpath ./rest-api/data --quiet",
    "mongo-daemon": "mongod --bind_ip localhost --dbpath ./rest-api/data --fork --logpath logs/mongo.log",
    "process-hosts": "babel-node ./bin/process-hosts.js",
    "process-netflows": "babel-node ./bin/process-netflows.js",
    "rest-api": "NODE_ENV=production nodemon ./rest-api --exec babel-node",
    "rest-api-daemon": "NODE_ENV=production forever start -c babel-node ./rest-api",
    "server": "yarn mongo-daemon && yarn rest-api-daemon",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "adj-noun": "^0.1.1",
    "autoprefixer": "6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-lodash": "^3.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-app": "^1.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "body-parser": "^1.15.2",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.25.0",
    "d3": "^4.3.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "^3.10.0",
    "eslint-import-resolver-babel-root-import": "^0.0.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "event-stream": "^3.3.4",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "forever": "^0.15.3",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "ip": "^1.1.4",
    "jest": "16.0.2",
    "json-loader": "0.5.4",
    "lodash": "^4.16.6",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.7",
    "morgan": "^1.7.0",
    "multistream": "^2.1.0",
    "node-restful": "^0.2.5",
    "node-sass": "^3.11.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react": "15.4.0-rc.4",
    "react-dev-utils": "^0.3.0",
    "react-dom": "15.4.0-rc.4",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
