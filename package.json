{
  "name": "cmsc-636-project",
  "version": "1.0.0",
  "description": "VizSec Semester Project for CMSC 636 / Data Visualization",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:esturcke/cmsc-636-project.git",
    "type": "git"
  },
  "author": "Erik J. Sturcke <erik@sturcke.co>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "process-hosts": "psql cmsc-636 -f schemas/host.sql && babel-node bin/process-hosts.js | psql cmsc-636",
    "process-flows": "psql cmsc-636 -f schemas/flow.sql && babel-node bin/process-flows.js | psql cmsc-636",
    "process-intrusions": "psql cmsc-636 -f schemas/intrusion.sql && babel-node bin/process-intrusions.js | psql cmsc-636",
    "rest-api": "postgrest postgres://anon@localhost:5432/cmsc-636 -p 3001 -a anon -m 50000 -j `pwgen -s 64 1`",
    "rest-api-daemon": "nohup yarn run rest-api >> logs/postgrest.log 2>&1 &",
    "server": "yarn rest-api-daemon",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "push": "rsync -avz -e ssh build/ maiholz:~/www/"
  },
  "dependencies": {
    "@typopro/web-fira-sans": "^3.4.9",
    "adj-noun": "^0.1.1",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-lodash": "^3.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-app": "^2.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "d3": "^4.4.0",
    "detect-port": "^1.0.6",
    "dotenv": "2.0.0",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.8.0",
    "event-stream": "^3.3.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "forever": "^0.15.3",
    "fs-extra": "^1.0.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "^2.24.1",
    "http-proxy-middleware": "0.17.2",
    "ip": "^1.1.4",
    "javascript-natural-sort": "^0.7.1",
    "jest": "^17.0.3",
    "json-loader": "0.5.4",
    "lodash": "^4.16.6",
    "minireset.css": "^0.0.2",
    "morgan": "^1.7.0",
    "multistream": "^2.1.0",
    "node-sass": "^3.11.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.0",
    "path-exists": "^3.0.0",
    "pg": "^6.1.0",
    "pg-copy-streams": "^1.2.0",
    "postcss-loader": "^1.2.0",
    "promise": "7.1.1",
    "react": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dev-utils": "^0.4.0",
    "react-dom": "^15.4.1",
    "react-select": "^1.0.0-rc.2",
    "react-virtualized": "8.8.0-alpha.1",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
